PROC main()
\end_layout

\begin_layout Plain Layout

	IF diBr1=1 THEN
\end_layout

\begin_layout Plain Layout

		Trekant;
\end_layout

\begin_layout Plain Layout

	ELSEIF diBr2=1 THEN
\end_layout

\begin_layout Plain Layout

		Sirkel;
\end_layout

\begin_layout Plain Layout

	ELSEIF diBr3=1 THEN
\end_layout

\begin_layout Plain Layout

		Firkant;
\end_layout

\begin_layout Plain Layout

	ENDIF
\end_layout

\begin_layout Plain Layout

ENDPROC
\end_layout

\end_inset


\end_layout

